{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Mail": {
    "Mode": "smtp",
    "Smtp": {
      "Host": "${SMTP_HOST}",
      "Port": "${SMTP_PORT}",
      "User": "${SMTP_USER}",
      "Pass": "${SMTP_PASS}",
      "From": "${SMTP_FROM}"
    }
  },
  "Calendly": {
    "DefaultUrl": "${CALENDLY_USER_URL}"
  }
}
